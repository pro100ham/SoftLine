  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@crmAF">
            <Value>=Parameters!crmAF.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET                TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
                             SELECT        so.name, so.new_dateofcontract, su.title, su.new_namegenitiveukr, su.new_operatesbasis, so.owneridname, so.customeridname, ac.new_egrpou, ac.new_position, 
                                                       ac.new_authorizedpersonorganizationname, ac.new_operatesonthebasis, ac.emailaddress1, ac.new_actualindex, ac.new_cityname, ac.new_actualstreethouseoffice, ac.address2_postalcode, 
                                                       ac.address2_line2, ac.new_expense, ac.new_bank, ac.new_bankmfo, ac.new_svidnds, ac.new_inn, ac.telephone1, ac.fax, c.jobtitle, so.new_signatoryklientname, isnull(sod.productidname, 
                                                       sod.productdescription) AS 'Продукт', cast(sod.quantity AS int) AS 'Количество', sod.priceperunit, sod.baseamount, so.totallineitemamount, isnull(pr.productnumber,'') as productnumber, dbo.NumParse(CONVERT(BIGINT, 
                                                       so.totallineitemamount), 0) + ' грн. ' + RIGHT(so.totallineitemamount, 2) + ' коп.' AS ConvertNumber, Replace(so.totallineitemamount, '.', ' грн. ') + ' коп.' AS PriceInFormat, 
                                                       so.totallineitemamount / ex.new_megbank AS PriceInUSD, Replace(ex.new_megbank, '.', ' грн. ') + ' коп.' AS CurrensyUSD, so.freighttermscodename
                              FROM            FilteredSalesOrder so JOIN
                                                       FilteredSystemUser su ON so.ownerid = su.systemuserid JOIN
                                                       FilteredAccount ac ON so.customerid = ac.accountid JOIN
                                                       FilteredContact c ON c.contactid = so.new_signatoryklient JOIN
                                                       FilteredSalesOrderDetail sod ON so.salesorderid = sod.salesorderid JOIN
                                                       Filterednew_exchangerates ex ON FORMAT(ex.new_date, 'yyyy-MM-dd 00:00:00.000') = so.new_dateofcontract left JOin
													   FilteredProduct pr on pr.productId = sod.productId
                              WHERE        so.salesorderid = @crmAF AND ex.new_name = 'UAH'</CommandText>
      </Query>
