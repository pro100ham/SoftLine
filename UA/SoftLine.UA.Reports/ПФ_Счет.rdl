        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
select bank.new_schet,
		bank.new_mfo,
		bank.new_name,
		inv.customeridname,
		inv.salesorderidname,
		inv.name,
		acc.telephone1,
		acc.primarycontactidname,
		Format(inv.new_dateinvoice,'dd-MM-yyyy') as new_dateinvoice,
		isnull(invD.new_commentname,'')+' '+isnull(invD.productidname,invD.productdescription) as product,
		CAST(invD.quantity as int) as quantity,
		invD.priceperunit -	isnull(invD.tax/invD.quantity, 0) as priceperunit,
		invD.baseamount - isnull(invD.tax,0) as baseamount,
		inv.totallineitemamount - isnull(inv.totaltax,0) as pricewithouttax,
		inv.totallineitemamount,
		inv.owneridname,
		cast(inv.totaltax as decimal(10,0)) as totaltax,
		dbo.NumParse(CAST(inv.totallineitemamount as decimal(15,2)),0) as Parse,
		RIGHT(inv.totallineitemamount,2) as cent,
		Format(dateadd(day,2,inv.new_dateinvoice),'dd-MM-yyyy') as new_dateinvoice2
from FilteredInvoice inv
join Filterednew_bank bank
on inv.new_bank = bank.new_bankid
join FilteredInvoiceDetail invD
on inv.invoiceid = invD.invoiceid
join FilteredAccount acc
on acc.accountid = inv.customerid
where inv.invoiceid = @crmAF</CommandText>
      </Query>
