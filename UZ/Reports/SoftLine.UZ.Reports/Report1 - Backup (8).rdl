          <DataField>Оборот2</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Source</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
declare @summ decimal = (select SUM(isnull(op.new_summa,0)) from Filterednew_oplata op where op.new_data_oplati &gt; @from and op.new_data_oplati &lt; @to);
declare @mar decimal = (select SUM(isnull(op.new_marjausd,0)) from Filterednew_oplata op where op.new_data_oplati &gt; @from and op.new_data_oplati &lt; @to);

select isnull(sod.productidname, sod.productdescription) as 'Продукт',
		isnull(sod.new_manufacturingnamename, 'Не указано') as 'Вендор',
		CAST(((isnull(Sum(op.new_marjausd)/(select count (*)
						from FilteredSalesOrderDetail sod
						where sod.salesorderid = so.salesorderid),1))) / ((isnull(Sum(isnull(op.new_summa,0))/(select count (*)
						from FilteredSalesOrderDetail sod
						where sod.salesorderid = so.salesorderid),1) ))*100 as DECIMAL(16,2)) as 'маржи от оборота',
		CAST(((isnull(Sum(isnull(op.new_marjausd,0))/(select count (*)
						from FilteredSalesOrderDetail sod
						where sod.salesorderid = so.salesorderid),1) / @mar)*100) as DECIMAL(16,2)) as 'Маржа1',
		CAST(((isnull(Sum(isnull(op.new_summa,0))/(select count (*)
						from FilteredSalesOrderDetail sod
						where sod.salesorderid = so.salesorderid),1) / @summ)*100) as DECIMAL(16,2)) as 'Оборот1',
		CAST(isnull(Sum(isnull(op.new_marjausd,0))/(select count (*)
						from FilteredSalesOrderDetail sod
						where sod.salesorderid = so.salesorderid),1) as DECIMAL(16,2)) as 'Маржа2',
		CAST(isnull(Sum(isnull(op.new_summa,0))/(select count (*)
						from FilteredSalesOrderDetail sod
						where sod.salesorderid = so.salesorderid),1) as DECIMAL(16,2))   as 'Оборот2'
From FilteredSalesOrder so
join Filterednew_oplata op
on op.new_dogovorid = so.salesorderid
join FilteredSalesOrderDetail sod
on sod.salesorderid = so.salesorderid
where op.new_data_oplati &gt; @from and op.new_data_oplati &lt; @to
Group by sod.productidname, 
		sod.productdescription,
		sod.new_manufacturingnamename,
		--op.new_data_oplati,
		op.new_dogovorid,
		so.salesorderid</CommandText>
