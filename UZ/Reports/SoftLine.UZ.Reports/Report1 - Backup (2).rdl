  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Продукт">
          <DataField>Продукт</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID__маржи_от_оборота">
          <DataField>% маржи от оборота</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Sum_of_Маржа___">
          <DataField>Sum of Маржа, $</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Sum_of_Оборот___">
          <DataField>Sum of Оборот, $</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
select isnull(sod.productidname, sod.productdescription) as 'Продукт',
		CAST(((isnull(Sum(op.new_marjausd),1))) / ((isnull(Sum(op.new_summa),1) ))*100 as DECIMAL(16,2)) as '% маржи от оборота',
		CAST(((isnull(Sum(op.new_marjausd),1))) as DECIMAL(16,2)) as 'Sum of Маржа, $',
		CAST(((isnull(Sum(op.new_summa),1) )) as DECIMAL(16,2))   as 'Sum of Оборот, $'
From FilteredSalesOrder so
join Filterednew_oplata op
on op.new_dogovorid = so.salesorderid
join FilteredSalesOrderDetail sod
on sod.salesorderid = so.salesorderid
where so.new_contract_date &gt; @from and so.new_contract_date &lt; @to
Group by sod.productidname, sod.productdescription</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@from">
            <Value>=Parameters!from.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@to">
            <Value>=Parameters!to.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
