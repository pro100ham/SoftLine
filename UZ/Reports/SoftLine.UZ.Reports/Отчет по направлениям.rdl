  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Source</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@from">
            <Value>=Parameters!from.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@to">
            <Value>=Parameters!to.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
select isnull(sod.new_producttypecodename,'Не указано') as 'Направление',
		sum(op.new_summa) as 'Оборот',
		sum(op.new_marjausd) as 'Маржа'
from FilteredSalesOrder so
JOIN FilteredSalesOrderDetail sod
ON so.salesorderid = sod.salesorderid
JOIN Filterednew_oplata op
ON so.salesorderid = op.new_dogovorid
where op.new_data_oplati &gt; @from and op.new_data_oplati &lt; @to
group by new_producttypecodename</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
