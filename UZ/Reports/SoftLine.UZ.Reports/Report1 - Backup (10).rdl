        </Field>
        <Field Name="Sum_of_Оборот___">
          <DataField>Sum of Оборот, $</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Sum_of_Закуп___">
          <DataField>Sum of Закуп, $</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Sourse</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select isnull(sod.productidname, sod.productdescription) as 'Продукт',
		sod.new_manufacturingnamename as 'Вендор',
		so.customeridname as 'Клиент',
		so.new_agreem_number as '№ инвойса',
		so.new_contract_date as 'Дата',
		so.new_dop_zatratu_money as 'Ребейт, $',
		so.owneridname as 'Менеджер',
		Sum(op.new_marjausd) as 'Sum of Маржа, $',
		Sum(op.new_summa) as 'Sum of Оборот, $',
		Sum(op.new_summa - op.new_marjausd) as 'Sum of Закуп, $'
From FilteredSalesOrder so
join Filterednew_oplata op
on op.new_dogovorid = so.salesorderid
join FilteredSalesOrderDetail sod
on sod.salesorderid = so.salesorderid
where op.new_data_oplati &gt; @from and op.new_data_oplati &lt; @to
Group by sod.productidname, sod.productdescription,
		sod.new_manufacturingnamename,
		so.customeridname,
		so.new_agreem_number,
		so.new_contract_date,
		so.new_dop_zatratu_money,
		so.owneridname</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@from">
            <Value>=Parameters!from.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@to">
            <Value>=Parameters!to.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
